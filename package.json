{
  "name": "designxcel",
  "version": "1.0.0",
  "description": "Modern furniture e-commerce platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "test": "cd frontend && npm test",
    "lint": "cd frontend && npm run lint",
    "clean": "rimraf node_modules frontend/node_modules backend/node_modules frontend/build",
    "postinstall": "npm run install:all"
  },
  "keywords": [
    "ecommerce",
    "furniture",
    "react",
    "nodejs",
    "express"
  ],
  "author": "DesignXcel Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EugenioPantua19/DesignXcel.git"
  },
  "bugs": {
    "url": "https://github.com/EugenioPantua19/DesignXcel/issues"
  },
  "homepage": "https://github.com/EugenioPantua19/DesignXcel#readme"
}